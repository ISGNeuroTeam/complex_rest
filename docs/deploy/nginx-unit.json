{
    "listeners": {
        "*:50000": {
            "pass": "applications/complex_rest"
        }
    },

    "applications": {
        "complex_rest": {
            "type": "python 3.9",
            "home": "./venv/",
            "path": "./complex_rest",
            "module": "core.wsgi",
            "environment": {
                "DJANGO_SETTINGS_MODULE": "core.settings",
            },
            "limits": {
                "timeout": 60
            },
            "processes": 10
        }
    }
}