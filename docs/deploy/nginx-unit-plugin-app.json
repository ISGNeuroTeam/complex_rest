{
  "type": "python 3.9",
  "home": "./venv/",
  "path": "./complex_rest",
  "module": "core.wsgi",
  "environment": {
      "DJANGO_SETTINGS_MODULE": "core.settings",
      "PLUGIN_NAME": "{{plugin_name}}"
  },
  "limits": {
      "timeout": 60
  },
  "processes": {
    "max": 10,
    "spare": 3,
    "idle_timeout": 20
  }
}